INSERT INTO SPR_PO
	VALUES ("7caee1fa-6ea1-47a6-b17a-aab8f48e7658",
	'post_op',
	'',
	'// create the request
sync_id = UTIL::id_factory();
create object instance request of request;
data = raw_data::duplicate( raw_data:param.data );
generate request1:create( data:data, target:param.target, origin:param.origin, inbound:false, sync_id:sync_id, is_reply:false ) to request;

// wait for the return request
select one return_request related by request->request[R6519.''has return''];
while ( empty return_request )
  error = OK;
  ret_val = SYS::sleep( milliseconds:100, error:error );
  if ( -1 == ret_val )
    UTIL::report_error( code:error, level:error_levels::fatal );
    return param.data;
  end if;
  select one return_request related by request->request[R6519.''has return''];
end while;

// cache data to deliver
return_raw_data = return_request.data;
return_data = return_raw_data.toraw_data();

// clean up
return_request.dispose();
generate request4:done() to request;

// return
return return_data;',
	1,
	0);
