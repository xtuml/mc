	'initialize',
	'',
	'server_port = 0;
backlog = 0;

values[7] = "";
done = false;
while ( not done )
  // Read a line from the config file
  count = CSV::readline( filename:"socket.config", values:values );
  if ( count > 0 )
    if ( count == 2 ) // valid non-comment line
      if ( "server_port" == values[0] )
        server_port = STRING::atoi( s:STRING::trim( s:values[1] ) );
      elif ( "backlog" == values[0] )
        backlog = STRING::atoi( s:STRING::trim( s:values[1] ) );
      end if;
    end if;
  else
    done = true;
  end if;
end while;

generate server1:start( name:param.name, port:server_port, backlog:backlog ) to server creator;',
	1,
	0);
