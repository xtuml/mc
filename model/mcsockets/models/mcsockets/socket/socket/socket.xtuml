INSERT INTO SPR_PS
	VALUES ("56eaddcf-119d-4b1d-ab40-c938273ac24d",
	'initialize',
	'',
	'server_host = "";
server_port = 0;
timeout = 0;

values[7] = "";
done = false;
while ( not done )
  // Read a line from the config file
  count = CSV::readline( filename:"socket.config", values:values );
  if ( count > 0 )
    if ( count == 2 ) // valid non-comment line
      if ( "server_host" == values[0] )
        server_host = STRING::trim( s:values[1] );
      elif ( "server_port" == values[0] )
        server_port = STRING::atoi( s:STRING::trim( s:values[1] ) );
      elif ( "timeout" == values[0] )
        timeout = STRING::atoi( s:STRING::trim( s:values[1] ) );
      end if;
    end if;
  else
    done = true;
  end if;
end while;

generate client1:start( name:param.name, peer_name:param.peer_name, host:server_host, port:server_port, timeout:timeout ) to client creator;',
	1,
	0);
