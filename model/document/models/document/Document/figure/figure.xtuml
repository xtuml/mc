	1,
	'select any doc_fig from instances of DOC_FIG;
doc_fig = self;
T::include( file:"docbook/t.figurebegin.xml" );
select one doc_img related by self->DOC_IMG[R2313];
if ( not_empty doc_img )
  doc_img.render();
