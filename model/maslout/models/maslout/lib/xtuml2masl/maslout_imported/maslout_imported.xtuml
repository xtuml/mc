      descrip_begin = STRING::substr( s:o_obj.Descrip, begin:0, end:i );
      descrip_end = STRING::substr( s:o_obj.Descrip, begin:j+14, end:-1 );
      o_obj.Descrip = descrip_begin + descrip_end;
    end if;

    out::populate( element:"identifier", value:emptyvalue );
  end if;
end for;
