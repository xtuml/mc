  end if;
  var_name = te_var.buffer;
  T::include(file:"c/t.smt.generate.c");
  te_smt.OAL = "GENERATE ${sm_evt.Drv_Lbl}:${sm_evt.Mning}(${parameter_OAL}) TO ${v_var.Name}";
