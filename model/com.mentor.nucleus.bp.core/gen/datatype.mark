.print "data type mark"
.select any s_dt from instances of S_DT where ( selected.Name == "stringpointer" )
.select many o_attrs from instances of O_ATTR where ( selected.Name == "Action_Semantics_internal" )
.for each o_attr in o_attrs
  .assign o_attr.DT_ID = 0
  .assign o_attr.DT_ID = s_dt.DT_ID
.end for
.invoke MapDataTypeAsPointer( "*", "stringpointer", "c_t", "" )
